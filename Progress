public static void Progress()
        {
            string e1 = "Press Ups";
            string e2 = "Sit Ups";
            string e3 = "Mountain Climbers";   
            string e4 = "Squats";
            decimal startWeight = 0;
            decimal endGoal = 0;

            Console.WriteLine("What is your start weight?");
            startWeight = Convert.ToDecimal(Console.ReadLine());

            Console.WriteLine("What is your end goal?");
            endGoal = Convert.ToDecimal(Console.ReadLine());
            decimal change = startWeight - endGoal;
            if (change > 0)
            {
                Console.WriteLine(e1 + " - " + (change * 3 / 2));
                Console.WriteLine(e2 + " - " + change);
                Console.WriteLine(e3 + " - " + (change * 5/2));
                Console.WriteLine(e4 + " - " + change);
            }
        }
